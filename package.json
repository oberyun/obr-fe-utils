{
  "name": "@obr-fe/utils",
  "version": "1.0.21",
  "description": "Generic JavaScript utility functions by Beijing Oubeier Software.",
  "author": "oberyun",
  "license": "MIT",
  "homepage": "https://dev-fe.oberyun.com/fe-docs/obr-utils/",
  "repository": "https://github.com/oberyun/obr-fe-utils",
  "bugs": {
    "url": "https://github.com/oberyun/obr-fe-utils/issues"
  },
  "keywords": [
    "utils",
    "axios",
    "typescript",
    "vue",
    "lodash"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/**/*.d.ts",
      "import": "./dist/**/*.js",
      "require": "./dist/**/*.cjs"
    },
    "./dist/*": {
      "types": "./dist/**/*.d.ts",
      "import": "./dist/**/*.js",
      "require": "./dist/**/*.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "fix": "eslint . --fix",
    "publish": "npm publish --registry https://registry.npmjs.org/"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "lodash.camelcase": "^4.3.0",
    "lodash.clone": "^4.5.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.kebabcase": "^4.1.1",
    "lodash.merge": "^4.6.2",
    "lodash.set": "^4.3.2",
    "pinyin-pro": "^3.26.0",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash.camelcase": "^4.3.9",
    "@types/lodash.clone": "^4.5.9",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.has": "^4.5.9",
    "@types/lodash.kebabcase": "^4.1.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/lodash.set": "^4.3.9",
    "@types/node": "^20.12.12",
    "eslint": "^9.15.0",
    "fast-glob": "^3.3.1",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vitepress": "^1.0.2"
  }
}
